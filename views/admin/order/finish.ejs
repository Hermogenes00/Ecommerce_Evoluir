<%- include('../../partials/header') %>
<%- include('../../partials/navbarhome') %>




<div class="container-fluid mt-3">
    <div class="card">
        <div class="card-header">Cliente</div>
        <div class="card-body">
            <div class="row">
                <div class="col">Nome <%= result.cliente.nome %> </div>
                <div class="col">Cpf <%= result.cliente.cnpjCpf %></div>
                <div class="col">Email <%= result.cliente.email %></div>
            </div>
            <div class="row">
                <div class="col">Contato <%= result.cliente.tel %></div>
                <div class="col">Tel <%= result.cliente.cel1 %></div>
                <div class="col">Tel <%= result.cliente.cel2 %></div>
            </div>
        </div>
        <div class="card-footer">Rodapé</div>
    </div>
    <div class="card mt-3">
        <div class="card-header">Pedido</div>
        <div class="card-body">
            <div class="row">
                <div class="col">Total: <%= result.pedido.total %> </div>
                <div class="col">Frete: <%= result.pedido.valorFrete %> </div>
                <div class="col">Tel: <%= result.pedido.createdAt %></div>
            </div>
            <hr>
            <h6 class="text-center">Itens do Pedido</h6>
            <div class="table-responsive container">
                <table class="table table-borderless">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">First</th>
                            <th scope="col">Last</th>
                            <th scope="col">Handle</th>
                            <th scope="col">First</th>
                            <th scope="col">Last</th>
                            <th scope="col">Handle</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row">1</th>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>@mdo</td>
                            <td>Mark</td>
                            <td>Otto</td>
                            <td>@mdo</td>
                        </tr>
                    </tbody>
                </table>
            </div>

        </div>
        <div class="card-footer">Rodapé</div>
    </div>
    <div class="card mt-3">
        <div class="card-header">Endereço</div>
        <div class="card-body">
            Corpo
        </div>
        <div class="card-footer">Rodapé</div>
    </div>
</div>

<%- include('../../partials/footer') %>

<script src="/js/ajax.js"></script>