<%- include('../../partials/header') %>
<%- include('../../partials/navbaradmin') %>


<div class="container-fluid bg-warning" style="padding-bottom: 5px; padding-top: 5px;">

    <div class="row text-center">
        <div class="col">
            <h6>Aguardando</h6>
        </div>
        <div class="col">
            <h6>Produzindo</h6>
        </div>
        <div class="col">
            <h6>Finalizado</h6>
        </div>
    </div>

    <ul class="list-group">
        <div class="row">
            <div class="col-md-4 area" style="padding-left: 5px;">
                <% itens.forEach(item=>{ %>
                    <li class="list-group-item" data-idOrder="<%=item.pedido.id%> " draggable="true">
