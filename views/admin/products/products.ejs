<%- include('../../partials/header') %>
<%- include('../../partials/navbarhome') %>



<div class="container mt-4">



    <table class="table table-hover table-sm">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">NOME</th>
                <th scope="col">VALOR</th>
                <th scope="col">TAMANHO C/SANGRIA</th>
                <th scope="col">TAMANHO FINAL</th>
                <th scope="col">AÇÃO</th>
            </tr>
        </thead>
        <tbody>
            <% products.forEach(product=>{ %>
            <tr>
                <th scope="row"><%= product.id %></th>
                <td><%= product.nome %></td>
                <td><%= product.vlrProduto %></td>
                <td><%= product.tamSangria %></td>
                <td><%= product.tamFinal %></td>
                <td>
                    <a href="/admin/products/edit/<%= product.id %>" class="btn btn-outline-warning btn-sm">Editar</a>
                    <a href="" class="btn btn-outline-danger btn-sm">Excluir</a>
                </td>
            </tr>
            <%}) %>
        </tbody>

    </table>

</div>








<script src="/js/ajax.js"></script>

<%- include('../../partials/footer') %>